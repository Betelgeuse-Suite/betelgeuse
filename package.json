  "devDependencies": {
    "ava": "^0.22.0"
  },
  "ava": {
    "failFast": true,
    "tap": false
  },
  "scripts": {
    "clean:build": "rm -rf ./.bin ./build",
    "clean:tests": "rm -rf ./build_test",
    "clean:release": "rm -rf ./.bin",
    "build": "npm run clean:build && tsc",
    "build:watch": "npm run build -- --watch",
    "test": "npm run clean:tests && tsc -p tsconfig.test.json && ava",
    "test:watch": "tsc -p tsconfig.test.json --watch & ava --watch",
    "start": "npm run test:watch",
    "release": "npm run clean:release && tsc -p tsconfig.release.json"
  }
